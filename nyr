export const sortCasasOnCurrency = (currencyPrev, currencyNext) => {
  const sortCurrencyOrder = ["AED", "USD", "GBP", "EUR"];
  const currencyOne = currencyPrev["currency-code"];
  const currencyTwo = currencyNext["currency-code"];

  const indexOne = sortCurrencyOrder.indexOf(currencyOne);
  const indexTwo = sortCurrencyOrder.indexOf(currencyTwo);

  if (indexOne === -1) {
      return 1;
  } else if (indexTwo === -1) {
      return -1;
  } else {
      return indexOne - indexTwo;
  }
}
