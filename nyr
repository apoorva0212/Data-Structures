const apiResponse = [
    // ... your API response data ...
];

// Define the custom sorting order
const customCurrencyOrder = ["AED", "INR", "GBP", "EUR"];

// Custom comparison function to sort by currency
function customCurrencySort(a, b) {
    const currencyA = a["currency-code"];
    const currencyB = b["currency-code"];
    
    // Check if currencies are in the custom order
    const indexA = customCurrencyOrder.indexOf(currencyA);
    const indexB = customCurrencyOrder.indexOf(currencyB);
    
    if (indexA === -1 && indexB === -1) {
        // If both currencies are not in the custom order, keep their original order
        return 0;
    } else if (indexA === -1) {
        // If currencyA is not in the custom order, move it to the end
        return 1;
    } else if (indexB === -1) {
        // If currencyB is not in the custom order, move it to the end
        return -1;
    } else {
        // Compare the positions in the custom order
        return indexA - indexB;
    }
}

// Sort the API response using the custom comparison function
const sortedApiResponse = apiResponse.sort(customCurrencySort);

// Output the sorted API response
console.log(sortedApiResponse);
